{
    "name": "bookswap_backend",
    "scripts": {
        "start": "node ./dist/server.js",
        "build": "tsc -p tsconfig.prod.json && tsc-alias",
        "build:watch": "npm run build && (concurrently \"tsc -p tsconfig.prod.json -w\" \"tsc-alias -w\")",
        "build:docker": "docker build -t bookswap_backend:latest . -f Dockerfile",
        "dev": "nodemon",
        "debug": "set DEBUG=* && nodemon",
        "format": "prettier --write ./src",
        "lint": "eslint . --ext .js,.ts,.json",
        "lint-fix": "eslint --fix . --ext .js,.ts,.json",
        "test": "jest || ECHO."
    },
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "connect-mongo": "^4.6.0",
        "cors": "^2.8.5",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "mongoose": "6.7.2",
        "morgan": "^1.10.0",
        "socket.io": "^4.5.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/jest": "^29.2.3",
        "@types/morgan": "^1.9.3",
        "@types/node": "^18.11.9",
        "@types/supertest": "^2.0.12",
        "@types/validator": "^13.7.10",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "concurrently": "^7.5.0",
        "debug": "^4.3.4",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "mongodb-memory-server": "^8.10.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "supertest": "^6.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.7.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.9.3"
    }
}
